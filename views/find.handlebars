<div class="container">
  <ul>
    <li>
      {{#each workout}}
      <div class="row">
        <div class="col s12 m6">
          <div class="card transparent">
            <div class="card-content indigo-text">
              <span class="card-title"> {{name}} </span>
              <button type="submit" data-id="{{id}}"
                class="btn-floating halfway-fab waves-effect waves-light indigo"><a
                  href="/workouts/{{id}}">
                  <i class="material-icons">search</i></a>
              </button>
              <p>{{type}}</p>
            </div>
          </div>
        </div>
      </div>
      {{/each}}
    </li>
  </ul>
</div>